{
  "info": {
    "name": "Project Uniview API",
    "description": "Complete API collection for Project Uniview smart parking system",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{accessToken}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://dev-api.uniview.com/api/v1",
      "type": "string"
    },
    {
      "key": "accessToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "refreshToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "userId",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', jsonData.refreshToken);",
                  "    pm.collectionVariables.set('userId', jsonData.userId);",
                  "    pm.test('Registration successful', function() {",
                  "        pm.expect(jsonData).to.have.property('userId');",
                  "    });",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@rutgers.edu\",\n  \"password\": \"SecurePass123!\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"phone\": \"+12345678901\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', jsonData.refreshToken);",
                  "    pm.collectionVariables.set('userId', jsonData.userId);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@rutgers.edu\",\n  \"password\": \"SecurePass123!\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Refresh Token",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.accessToken);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"{{refreshToken}}\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/auth/refresh",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "refresh"]
            }
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/auth/logout",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "logout"]
            }
          }
        }
      ]
    },
    {
      "name": "Parking Lots",
      "item": [
        {
          "name": "Get All Lots",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/lots?includeOffline=false&latitude=40.5231&longitude=-74.4587&radius=5000",
              "host": ["{{baseUrl}}"],
              "path": ["lots"],
              "query": [
                {
                  "key": "includeOffline",
                  "value": "false"
                },
                {
                  "key": "latitude",
                  "value": "40.5231"
                },
                {
                  "key": "longitude",
                  "value": "-74.4587"
                },
                {
                  "key": "radius",
                  "value": "5000"
                }
              ]
            }
          }
        },
        {
          "name": "Get Lot Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/lots/LOT_A",
              "host": ["{{baseUrl}}"],
              "path": ["lots", "LOT_A"]
            }
          }
        },
        {
          "name": "Get Lot Spaces",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/lots/LOT_A/spaces?status=AVAILABLE",
              "host": ["{{baseUrl}}"],
              "path": ["lots", "LOT_A", "spaces"],
              "query": [
                {
                  "key": "status",
                  "value": "AVAILABLE"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Predictions",
      "item": [
        {
          "name": "Get Lot Prediction",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/lots/LOT_A/prediction?hours=3",
              "host": ["{{baseUrl}}"],
              "path": ["lots", "LOT_A", "prediction"],
              "query": [
                {
                  "key": "hours",
                  "value": "3"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "User Preferences",
      "item": [
        {
          "name": "Get Preferences",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/users/me/preferences",
              "host": ["{{baseUrl}}"],
              "path": ["users", "me", "preferences"]
            }
          }
        },
        {
          "name": "Update Preferences",
          "request": {
            "method": "PUT",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"favoriteSpots\": [\"LOT_A\", \"LOT_C\"],\n  \"notifications\": true,\n  \"theme\": \"dark\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/users/me/preferences",
              "host": ["{{baseUrl}}"],
              "path": ["users", "me", "preferences"]
            }
          }
        }
      ]
    },
    {
      "name": "Admin",
      "item": [
        {
          "name": "Get All Sensors",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/admin/sensors?status=ONLINE&page=1&limit=50",
              "host": ["{{baseUrl}}"],
              "path": ["admin", "sensors"],
              "query": [
                {
                  "key": "status",
                  "value": "ONLINE"
                },
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "50"
                }
              ]
            }
          }
        },
        {
          "name": "Override Sensor Status",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"AVAILABLE\",\n  \"reason\": \"Maintenance testing\",\n  \"duration\": 3600\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/admin/sensors/NODE_001/override",
              "host": ["{{baseUrl}}"],
              "path": ["admin", "sensors", "NODE_001", "override"]
            }
          }
        },
        {
          "name": "Get Occupancy Analytics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/admin/analytics/occupancy?lotId=LOT_A&startDate=2026-01-01T00:00:00Z&endDate=2026-01-06T23:59:59Z&granularity=daily",
              "host": ["{{baseUrl}}"],
              "path": ["admin", "analytics", "occupancy"],
              "query": [
                {
                  "key": "lotId",
                  "value": "LOT_A"
                },
                {
                  "key": "startDate",
                  "value": "2026-01-01T00:00:00Z"
                },
                {
                  "key": "endDate",
                  "value": "2026-01-06T23:59:59Z"
                },
                {
                  "key": "granularity",
                  "value": "daily"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}
